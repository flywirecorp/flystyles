{
  "name": "flystyles",
  "version": "0.0.1",
  "description": "Flywire CSS Framework",
  "main": [
    "dist/main.css",
    "dist/main.js"
  ],
  "scripts": {
    "css:build": "node_modules/.bin/node-sass src/index.scss dist/main.css",
    "css:autoprefix": "node_modules/.bin/postcss --use autoprefixer -o dist/main.css dist/main.css",
    "build": "npm run css:build && npm run css:autoprefix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peertransfer/flystyles.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/peertransfer/flystyles/issues"
  },
  "homepage": "https://github.com/peertransfer/flystyles#readme",
  "devDependencies": {
    "node-sass": "^3.8.0"
  }
}
